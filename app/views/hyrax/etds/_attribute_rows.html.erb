<%= presenter.attribute_to_html(:description, label: t('simple_form.labels.etd.description'), render_as: :description, html_dl: true) %>
<%= presenter.attribute_to_html(:resource_type, label: t('hyrax.base.show.resource_type'), render_as: :faceted, html_dl: true) %>
<%= presenter.attribute_to_html(:creator, render_as: :faceted, html_dl: true) %>
<%= presenter.attribute_to_html(:contributor, render_as: :contributor, search_field: 'all_fields', html_dl: true) %>
<%= presenter.attribute_to_html(:abstract, render_as: :description, html_dl: true) %>
<%= presenter.attribute_to_html(:subject, render_as: :faceted, html_dl: true) %>
<%= presenter.attribute_to_html(:keyword, render_as: :faceted, html_dl: true) %>
<%= presenter.attribute_to_html(:language, render_as: :language, html_dl: true) %>
<%= presenter.attribute_to_html(:publisher, render_as: :faceted, html_dl: true) %>
<%= presenter.attribute_to_html(:degree_level, label: t('simple_form.labels.etd.degree_level'), render_as: :degree_level_faceted, html_dl: true) %>
<%= presenter.attribute_to_html(:degree, label: t('simple_form.labels.etd.degree'), render_as: :faceted, html_dl: true) %>
<%= presenter.attribute_to_html(:degree_discipline, label: t('simple_form.labels.etd.degree_discipline'), render_as: :faceted, html_dl: true) %>
<%= presenter.attribute_to_html(:identifier, html_dl: true) %>
<%= presenter.attribute_to_html(:based_near_label, html_dl: true) %>
<%= presenter.attribute_to_html(:related_url, label: t('hyrax.base.show.related_url'), html_dl: true, render_as: :external_link) %>
<%= presenter.attribute_to_html(:rights_statement, label: t('hyrax.base.show.rights_statement'), render_as: :rights_statement, html_dl: true) %>
<%= presenter.attribute_to_html(:access_right, label: t('hyrax.base.show.access_rights'), render_as: :external_link, html_dl: true) %>
<%= presenter.attribute_to_html(:rights_notes, label: t('hyrax.base.show.rights_notes'), render_as: :external_link, html_dl: true) %>
<%= presenter.attribute_to_html(:license, render_as: :license, html_dl: true) %>
<%= presenter.attribute_to_html(:date_created, label: t('hyrax.base.show.date_created'), render_as: :year_date, html_dl: true) %>
<% if can?(:edit, presenter) && !workflow_restriction?(presenter) %>
    <%= presenter.attribute_to_html(:date_modified, label: t('hyrax.base.show.last_modified') + " (Admin only)", html_dl: true) %>
    <%= presenter.attribute_to_html(:date_uploaded, label: "Date Uploaded (Admin only)", html_dl: true) %>
    <%= presenter.attribute_to_html(:internal_note, label: t('hyrax.etd.internal_note'), render_as: :description, html_dl: true) %>
    <%= presenter.attribute_to_html(:agreement, label: t('hyrax.etd.agreement'), html_dl: true, render_as: :agreement) %>
<% end %>

