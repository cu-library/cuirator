<%= presenter.attribute_to_html(:date_uploaded, label: "Date Uploaded (Admin only)", html_dl: true) %>
<%= presenter.attribute_to_html(:internal_note, html_dl: true) %>
<%= presenter.attribute_to_html(:agreement, render_as: :agreement, html_dl: true) %>
